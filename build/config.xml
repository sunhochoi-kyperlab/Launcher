<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" id="http://www.obigo.com/posco/C300/launcher" version="1.0.7" viewmodes="fullscreen">
  <name xml:lang="en-us" short="launcher">Launcher</name>
  <description xml:lang="en-us">Home for various applications. You can enjoy and explore the variety of apps in your car!</description>
  <icon src="icon.png"/>
  <thumbnail src="thumbnail.png" />
  <author>Obigo</author>
  <license>Obigo</license>
  <content src="index.html" type="text/html"/>
  <access origin="*"/>
  <feature name="http://www.obigo.com/api/applicationmanager" required="true">
    <param name="version" value="3.1.5.6" />
  </feature>
  <feature name="http://www.obigo.com/api/SVG" required="true">
    <param name="version" value="1.1.0.0"/>
  </feature>
  <feature name="http://www.obigo.com/api/storage" required="true">
      <param name="version" value="1.1.0.0"/>
  </feature>
  <meta-data>
    <manifest-version>1.3.2.1</manifest-version>
  </meta-data>
</widget>

